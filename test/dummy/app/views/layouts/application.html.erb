<!DOCTYPE html>
<html>
<head>
  <title>ElsSession</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if @els_identity %>
	<div class='els_logout'>
		<%= button_to "logout", els_session_destroy_path, :method => :delete %>
	</div>
<% end %>

<%= yield %>

</body>
</html>
